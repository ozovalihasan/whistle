<% if @rewhiistle.saved_changes? %>
  <%= turbo_stream.update whiistle_rewhiistle_button_target(@rewhiistle.whiistle) do %>
    <%= render "whiistles/destroy_rewhiistle_button", rewhiistle: @rewhiistle %>
  <% end %>

  <%= turbo_stream.update whiistle_rewhiistle_or_quote_button_target(@rewhiistle.whiistle) do %>
    <%= render "whiistles/destroy_rewhiistle_or_quote_button", whiistle: @rewhiistle.whiistle %>
  <% end %>
<% end %>

<%= turbo_stream.update "flashes" do %>
    <%= check_notice %>
    <%= check_alert %>
<% end %>