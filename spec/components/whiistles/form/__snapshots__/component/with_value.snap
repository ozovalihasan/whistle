<form class="simple_form w-full" id="new_whiistle" data-controller="flood" action="mock_url" accept-charset="UTF-8" method="post">
  <div data-controller="tabs" data-tabs-active-tab="-mb-px border-l border-t border-r rounded-t">
    <ul class="list-reset border-b" data-flood-target="add_item">
      <div class="flex ">
        <div class="m-2 shrink-0">
          Users::ProfileImage::Component(user: User)
        </div>
        
        <div class="mt-4 shrink w-full">
          <li class="-mb-px mr-1" data-tabs-target="tab" data-action="click->tabs#change">
            Whiistles::FormBody::Component(form: SimpleForm::FormBuilder, label: String, value: String, open_modal: NilClass, modal_path: NilClass)
          </li>
          <div class="hidden py-4 px-4 border-l border-b border-r" data-tabs-target="panel">
        
              <div class="w-full flex">
                Whiistles::FormPicturesButton::Component(form: SimpleForm::FormBuilder)
                <div class="ml-auto flex">


                  <input type="submit" name="commit" value="Whiistle" class="ml-auto btn-first py-2 px-4 my-4" data-disable-with="Whiistle" />
                </div>
              </div>
          </div>
        </div>
      </div>
    
    </ul>
  </div>

</form>